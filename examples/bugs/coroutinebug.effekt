type Coroutine[A] {
    Done()
    More(f: A => Coroutine[A] at {})
}

def createCoroutine(): Coroutine[String] = {
    var cr = Done();
    cr = More(
            fun(s:String){
                    Done()
                }
            )
    cr
}

def work{cr: => Coroutine[String]} =  {
    var coroutine = cr();
    coroutine
}

